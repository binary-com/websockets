{
   "$schema" : "http://json-schema.org/draft-04/schema#",
   "description" : "",
   "type" : "object",
   "required" : [
      "echo_req",
      "msg_type"
   ],
   "title" : "Transfer Between Accounts Response",
   
   "properties" : {
      "echo_req" : {
         "title" : "Echo request",
         "description" : "Echo of the request made"
      },
      "req_id" : {
         "type" : "integer",
         "description" : "Optional field send in request to map to response, present only when request contains req_id"
      },
      "client_to_full_name" : {
         "description" : "The account to client full name",
         "type" : "string"
      },
      "client_to_loginid" : {
         "description" : "The account to client loginid",
         "type" : "string"
      },
      "transfer_between_accounts" : {
         "enum" : [
            0,
            1
         ],
         "description" : "If 1, transfer success.",
         "type" : "integer"
      },
      "msg_type" : {
         "type" : "string",
         "description" : "transfer_between_accounts"
      },
      "accounts" : {
         "description" : "the available accounts to transfer",
         "type" : "array",
         "items" : {
            "properties" : {
               "currency" : {
                  "type" : "string"
               },
               "loginid" : {
                  "type" : "string"
               },
               "balance" : {
                  "type" : "string"
               }
            },
            "type" : "object"
         }
      },
      "transaction_id" : {
         "description" : "Reference id of transfer performed",
         "type" : "integer"
      }
   }
}
