{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Server Time (request)",
    "description": "Request back-end server epoch time.",
    "type": "object",
    "auth_required": 0,
    "additionalProperties": false,
    "required": [
        "time"
    ],
    "properties": {
        "time": {
            "description": "Must be `1`",
            "type": "integer",
            "enum": [
                1
            ]
        },
        "passthrough": {
            "description": "[Optional] Used to pass data through the websocket, which may be retrieved via the `echo_req` output field. Maximum size is 3500 bytes.",
            "type": "object"
        },
        "req_id": {
            "description": "[Optional] Used to map request to response.",
            "type": "integer"
        }
    }
}
