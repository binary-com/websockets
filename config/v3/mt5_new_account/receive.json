{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Create MT5 account Receive",
  "description": "Create MT5 account Receive",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made",
      "required": "1"
    },
    "mt5_new_account": {
      "type": "object",
      "title": "New MT5 account",
      "description": "New MT5 account details",
      "properties": {
        "login": {
          "type": "string",
          "description": "Login of new MT5 account"
        },
        "account_type": {
          "enum": ["demo", "gaming", "financial"],
          "description": "Account type."
        },
        "mt5_account_type": {
          "enum": ["cent", "standard", "stp"],
          "description": "Sub account type, present only when account type is financial."
        },
        "balance": {
          "type": "string",
          "description": "Account balance"
        }
      }
    },
    "msg_type": {
      "type": "string",
      "description": "mt5_new_account",
      "required": "1"
    },
    "req_id": {
      "type": "integer",
      "description": "Optional field send in request to map to response, present only when request contains req_id"
    }
  }
}
