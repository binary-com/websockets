{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "2-Factor Authentication (response)",
    "description": "The result of 2-Factor authentication request.",
    "type": "object",
    "required": [
        "echo_req",
        "msg_type"
    ],
    "properties": {
        "account_security": {
            "title": "account_security",
            "description": "The information of 2-Factor authentication.",
            "type": "object",
            "properties": {
                "totp": {
                    "description": "TOTP information.",
                    "type": "object",
                    "properties": {
                        "is_enabled": {
                            "description": "Determines whether the 2-Factor authentication is enabled.",
                            "type": "integer"
                        },
                        "secret_key": {
                            "description": "The secret key for the 2-Factor authentication.",
                            "type": "string"
                        }
                    }
                }
            }
        },
        "echo_req": {
            "description": "Echo of the request made.",
            "type": "object"
        },
        "msg_type": {
            "description": "Action name of the request made.",
            "type": "string",
            "enum": [
                "account_security"
            ]
        },
        "req_id": {
            "description": "Optional field sent in request to map to response, present only when request contains req_id.",
            "type": "integer"
        }
    }
}
