{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "Payout Currencies Receive",
  "properties": {
    "echo_req": {
      "title":"Echo request",
      "description":"Echo of the request made"
    },
    "payout_currencies": {
      "type": "array",
      "items": {
        "type": "string",
        "description": "Available payout currency (example: USD)"
      }
    },
    "msg_type":{
        "type":"string",
        "description":"payout_currencies"
